{
  "name": "butterfly-count-app",
  "id": "uk.ac.ceh.ebms",
  "title": "ButterflyCount",
  "description": "ButterflyCount mobile application.",
  "version": "1.6.1",
  "build": "1",
  "homepage": "http://www.butterfly-monitoring.net",
  "scripts": {
    "build": "webpack --display minimal",
    "build:production": "grunt",
    "start": "node other/server.js",
    "start:dev": "webpack-dev-server --config webpack.config.js --content-base dist/main --port 8000",
    "deploy": "vercel --prod -A other/now.json",
    "clean": "rm -rf cordova build resources",
    "test": "karma start test/karma.conf.js",
    "test:watch": "karma start test/karma.conf.js --no-single-run",
    "test:sauce": "karma start test/karma.conf-sauce.js",
    "test:eslint": "mocha test/eslint-test.js",
    "make:species:search": "cd src/common/data && node makeSpeciesSearch.js",
    "make:species:profiles": "cd src/common/data && node makeSpeciesProfiles.js",
    "fetch:species:names": "cd src/common/data && node fetchSpeciesCommonNames.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:eslint"
    }
  },
